<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoFit_Mobile_0914"
        android:usesCleartextTraffic="true"
        tools:targetApi="31" >
        <activity
            android:name=".introPage.StartUpActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".introPage.IntroPageActivity"
            android:exported="true" />
        <activity
            android:name=".mainmenu.HomeActivity"
            android:exported="true" />
        <activity
            android:name=".mainmenu.HomeInstructurActivity"
            android:exported="true" />
        <activity
            android:name=".mainmenu.HomeMOActivity"
            android:exported="true" />

        <activity
            android:name=".LoginActivity"
            android:exported="true" />

        <activity
            android:name="ProfileActivity"
            android:exported="true" />

    </application>

</manifest>